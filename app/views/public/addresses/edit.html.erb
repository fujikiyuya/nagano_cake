<h4>配送先編集</h4>
<table>
<%= form_with model: @address, url: address_path(@address.id), method: :update do |f| %>
  <tr>
    <td>郵便番号(ハイフンなし)</td>
    <td><%= f.text_field :postal_code %></td>
  </tr>
  <tr>
    <td>住所</td>
    <td><%= f.text_field :address %></td>
  </tr>
  <tr>
    <td>宛名</td>
    <td><%= f.text_field :name %></td>
    <td><%= f.submit "変更内容を保存する" %></td>
  </tr>
<% end %>
</table>
